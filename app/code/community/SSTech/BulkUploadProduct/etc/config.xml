<?xml version="1.0"?>
<config>
  <modules>
    <SSTech_BulkUploadProduct>
      <version>0.1.0</version>
    </SSTech_BulkUploadProduct>
  </modules>
  <frontend>
    <routers>
      <bulkuploadproduct>
        <use>standard</use>
          <args>
            <module>SSTech_BulkUploadProduct</module>
            <frontName>bulk-order</frontName> 
          </args>
      </bulkuploadproduct>
    </routers>
		<layout>
		  <updates>
			<bulkuploadproduct>
			  <file>bulkuploadproduct.xml</file>
			</bulkuploadproduct>
		  </updates>
		</layout>
  </frontend>
  <global>
    <helpers>
      <bulkuploadproduct>
        <class>SSTech_BulkUploadProduct_Helper</class>
      </bulkuploadproduct>
    </helpers>
	<blocks>
	  <bulkuploadproduct>
		<class>SSTech_BulkUploadProduct_Block</class>
	  </bulkuploadproduct>
	</blocks>
  </global>
    <default>
        <bulkuploadproduct>
            <functionality>
                <column>8,8,8,8</column>
            </functionality>
        </bulkuploadproduct>
    </default>
</config> 