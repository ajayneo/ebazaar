<config>
	<modules>
		<Neo_Customapiv2>
			<version>0.1.0</version>
		</Neo_Customapiv2>
	</modules>
	<frontend>
		<routers>
			<customapiv2>
				<use>standard</use>
				<args>
					<module>Neo_Customapiv2</module>
					<frontName>customapiv2</frontName>
				</args>
			</customapiv2>
		</routers>
	</frontend>
	<global>
		<models>
			<customapiv2>
				<class>Neo_Customapiv2_Model</class>
			</customapiv2>
		</models>
		<events>
			<customer_register_success>
				<observers>
					<customer_register_success>
						<class>Neo_Customapiv2_Model_Observer</class>
						<method>saveEncodedMd5Token</method>
					</customer_register_success>
				</observers>
			</customer_register_success>
			<adminhtml_customer_prepare_save>
				<observers>
					<adminhtml_customer_prepare_save>
						<class>Neo_Customapiv2_Model_Observer</class>
						<method>saveEncodedMd5Token</method>
					</adminhtml_customer_prepare_save>
				</observers>
			</adminhtml_customer_prepare_save>
		</events>
	</global>
