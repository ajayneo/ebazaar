<?xml version="1.0"?>
<config>
  <modules>
    <NeoCustom_CustomAjax>
      <version>0.1.0</version>
    </NeoCustom_CustomAjax>
  </modules>
  <frontend>
    <routers>
      <customajax>
        <use>standard</use>
          <args>
            <module>NeoCustom_CustomAjax</module>
            <frontName>customajax</frontName>
          </args>
      </customajax>
    </routers>
		<layout>
		  <updates>
			<customajax>
			  <file>customajax.xml</file>
			</customajax>
		  </updates>
		</layout>
  </frontend>
  <global>
    <helpers>
      <customajax>
        <class>NeoCustom_CustomAjax_Helper</class>
      </customajax>
    </helpers>
	<blocks>
	  <customajax>
		<class>NeoCustom_CustomAjax_Block</class>
	  </customajax>
	</blocks>
  </global>
</config> 