<?xml version="1.0"?>
<config>
    <modules>
        <Neo_Pdf>
            <version>0.1.0</version>
        </Neo_Pdf>
    </modules>
    <global>
        <models>
            <neo_pdf>
                <class>Neo_Pdf_Model</class>
            </neo_pdf>
            <sales>
                <rewrite>
                    <order_pdf_invoice>Neo_Pdf_Model_Sales_Order_Pdf_Invoice</order_pdf_invoice>
                    <order_pdf_items_invoice_default>Neo_Pdf_Model_Sales_Order_Pdf_Items_Invoice_Default</order_pdf_items_invoice_default>                    
                    <order_pdf_shipment>Neo_Pdf_Model_Sales_Order_Pdf_Shipment</order_pdf_shipment>
                    <order_pdf_items_shipment_default>Neo_Pdf_Model_Sales_Order_Pdf_Items_Shipment_Default</order_pdf_items_shipment_default>
                </rewrite>
            </sales>
            <tax>
                <rewrite>
                    <sales_pdf_tax>Neo_Pdf_Model_Sales_Order_Pdf_Tax</sales_pdf_tax>
                </rewrite>
            </tax>
            <bundle>
                <rewrite>
                    <sales_order_pdf_items_invoice>Neo_Pdf_Model_Bundle_Sales_Order_Pdf_Items_Invoice</sales_order_pdf_items_invoice>
                    <sales_order_pdf_items_shipment>Neo_Pdf_Model_Bundle_Sales_Order_Pdf_Items_Shipment</sales_order_pdf_items_shipment>
                </rewrite>
            </bundle>
        </models>      
        <helpers>
            <neo_pdf>
                <class>Neo_Pdf_Helper</class>
            </neo_pdf>
        </helpers>
        <blocks>
            <neo_pdf>
                <class>Neo_Pdf_Block</class>
            </neo_pdf>
        </blocks>
    </global>
</config>