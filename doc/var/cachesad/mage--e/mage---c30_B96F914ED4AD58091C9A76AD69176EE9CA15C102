            <div class="input-box">
               <input type="email" autocapitalize="off" autocorrect="off" spellcheck="false" name="email" id="newsletter" title="Your email id" placeholder="Your email id" class="input-text required-entry validate-email" />
               <div class="validation-advice" id="newsletter_status" style="display:none;"></div>
