    <script type="text/javascript">
    //<![CDATA[
        var newsletterSubscriberFormDetail = new VarienForm('newsletter-validate-detail');

        jQuery("#newsletter").keypress(function(e) {
            if(e.which == 13) {
                e.preventDefault();
                jQuery('#subscribe').focus().click();
            }
        });
        jQuery(document).ready(function() {
            jQuery('#subscribe').click(function() {
                if(newsletterSubscriberFormDetail.validator && newsletterSubscriberFormDetail.validator.validate()) {
                    var url = 'https://www.electronicsbazaar.com/innoviti/payment/newsletter/';
                    var email = jQuery('#newsletter').val();
                    jQuery.ajax({
                        url: url,
                        data: {email: email},
                        type: 'POST',
                        success: function(data) {
                            jQuery('#newsletter_status').html(data).css('display','block');
                        }
                    });
                } else {
                    newsletterSubscriberFormDetail.submit();
                }
            });
        });
    //]]>
    </script>
